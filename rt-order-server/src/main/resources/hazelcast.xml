<hazelcast xsi:schemaLocation="http://www.hazelcast.com/schema/config http://www.hazelcast.com/schema/config/hazelcast-config-3.3.xsd"
           xmlns="http://www.hazelcast.com/schema/config"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <network>
        <join>
            <multicast enabled="${multicast.enabled}"/>
            <aws enabled="${aws.enabled}">
                <access-key>${aws.access-key}</access-key>
                <secret-key>${aws.secret-key}</secret-key>
                <tag-key>role</tag-key>
                <tag-value>hz-server</tag-value>
            </aws>
        </join>
    </network>

    <serialization>
        <data-serializable-factories>
            <data-serializable-factory factory-id="1">com.prystupa.core.EventFactory</data-serializable-factory>
            <data-serializable-factory factory-id="2">com.prystupa.core.command.CommandFactory</data-serializable-factory>
        </data-serializable-factories>
    </serialization>

</hazelcast>
