<hazelcast xsi:schemaLocation="http://www.hazelcast.com/schema/config hazelcast-config-3.3.xsd"
           xmlns="http://www.hazelcast.com/schema/config"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <network>
        <join>
            <multicast enabled="${multicast.enabled}"/>
            <aws enabled="${aws.enabled}">
                <access-key>${aws.access-key}</access-key>
                <secret-key>${aws.secret-key}</secret-key>
                <tag-key>role</tag-key>
                <tag-value>hz-server</tag-value>
            </aws>
        </join>
    </network>

    <multimap name="chains">
        <value-collection-type>SET</value-collection-type>
        <entry-listeners>
            <entry-listener local="true" include-value="false">com.prystupa.core.EventChainListener</entry-listener>
        </entry-listeners>
    </multimap>

    <map name="parents">
        <entry-listeners>
            <entry-listener local="true" include-value="false">com.prystupa.core.ParentEventListener</entry-listener>
        </entry-listeners>
    </map>
</hazelcast>
