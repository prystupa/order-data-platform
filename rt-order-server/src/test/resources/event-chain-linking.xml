<hazelcast xsi:schemaLocation="http://www.hazelcast.com/schema/config http://www.hazelcast.com/schema/client-config/hazelcast-config-3.3.xsd"
           xmlns="http://www.hazelcast.com/schema/config"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <network>
        <join>
            <multicast enabled="false"/>
        </join>
    </network>

    <serialization>
        <data-serializable-factories>
            <data-serializable-factory factory-id="1">com.prystupa.core.EventFactory</data-serializable-factory>
        </data-serializable-factories>
    </serialization>

</hazelcast>
